    "node-sass": "^9.0.0",
    "postcss": "^8.4.38",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "7.7.2",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.7",
    "sass": "^1.74.1",
    "sass-loader": "^13.0.2",
    "storybook": "^8.1.5",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.1.8",
    "ts-essentials": "^7.0.1",
    "tsup": "^8.1.0",
    "typescript": "^5.3.3",
