{
  "license": "MIT",
  "name": "@seolhun/root-editor",
  "version": "0.1.3",
  "types": "dist/legacy/index.d.ts",
  "main": "dist/legacy/index.cjs",
  "module": "dist/legacy/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/modern/index.d.ts",
        "default": "./dist/modern/index.js"
      },
      "require": {
        "types": "./dist/modern/index.d.cts",
        "default": "./dist/modern/index.cjs"
      }
    },
    "./dist/index.css": "./dist/index.css",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/@seolhun"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "tailwind:build": "postcss public/tailwind.css -o dist/tailwind.css",
    "build:css": "postcss ./src/**/*.css --dir ./dist/css",
    "concat:css": "cat ./dist/css/*.css > ./dist/index.css",
    "prebuild": "rimraf dist/*",
    "build": "pnpm run build:css && pnpm run concat:css && tsup",
    "start": "node express.js",
    "start:dev": "webpack serve --config=build/webpack.dev.js",
    "dev": "node build/dev-server.js",
    "test": "vitest run",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "type:check": "tsc --noEmit",
    "prettier": "prettier --write '**/*.{ts,tsx,js,jsx}'",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build -o docs",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Seolhun@github.com:Seolhun/root-editor.git"
  },
  "keywords": [
    "react",
    "tailwind",
    "ui"
  ],
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": "> 0.25%, not dead",
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.17.0",
    "@lexical/clipboard": "0.15.0",
    "@lexical/code": "0.15.0",
    "@lexical/file": "0.15.0",
    "@lexical/hashtag": "0.15.0",
    "@lexical/headless": "^0.15.0",
    "@lexical/link": "0.15.0",
    "@lexical/list": "0.15.0",
    "@lexical/mark": "0.15.0",
    "@lexical/markdown": "^0.15.0",
    "@lexical/overflow": "0.15.0",
    "@lexical/plain-text": "0.15.0",
    "@lexical/react": "0.15.0",
    "@lexical/rich-text": "0.15.0",
    "@lexical/selection": "0.15.0",
    "@lexical/table": "0.15.0",
    "@lexical/text": "^0.15.0",
    "@lexical/utils": "0.15.0",
    "@lexical/yjs": "^0.15.0",
    "clsx": "^2.1.1",
    "katex": "^0.16.0",
    "lexical": "^0.15.0",
    "lodash-es": "^4.17.21",
    "react-error-boundary": "^4.0.13",
    "y-websocket": "^2.0.3",
    "yjs": "^13.6.15"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@changesets/cli": "^2.27.5",
    "@chromatic-com/storybook": "^1",
    "@mdx-js/react": "^2.1.2",
    "@seolhun/eslint-config": "^1.3.2",
    "@storybook/addon-actions": "^8.1.5",
    "@storybook/addon-docs": "^8.1.5",
    "@storybook/addon-essentials": "^8.1.5",
    "@storybook/addon-links": "^8.1.5",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/mdx2-csf": "^0.0.3",
    "@storybook/react": "^8.1.5",
    "@storybook/react-webpack5": "^8.1.5",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^16.14.14",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^8.3.0",
    "core-js": "^3.23.4",
