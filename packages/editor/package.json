{
  "license": "MIT",
  "name": "@seolhun/root-editor",
  "version": "0.1.7-alpha.12",
  "types": "dist/legacy/index.d.ts",
  "main": "dist/legacy/index.js",
  "module": "dist/legacy/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/modern/index.d.mts",
        "default": "./dist/modern/index.mjs"
      },
      "require": {
        "types": "./dist/legacy/index.d.ts",
        "default": "./dist/legacy/index.js"
      }
    },
    "./modern/index.css": "./dist/modern/index.css",
    "./legacy/index.css": "./dist/legacy/index.css",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist",
    "public",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "tailwind:build": "postcss public/tailwind.css -o dist/tailwind.css",
    "prebuild": "rimraf dist/*",
    "build": "tsup",
    "test": "vitest run",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "type:check": "tsc --noEmit",
    "prettier": "prettier --write '**/*.{ts,tsx,js,jsx}'",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build -o docs",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Seolhun@github.com:Seolhun/root-editor.git"
  },
  "keywords": [
    "react",
    "tailwind",
    "ui"
  ],
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": "> 0.25%, not dead",
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.17.0",
    "@floating-ui/react": "^0.26.18",
    "@fxts/core": "^1.0.0",
    "@heroicons/react": "^2.1.4",
    "@lexical/clipboard": "0.16.1",
    "@lexical/code": "0.16.1",
    "@lexical/file": "0.16.1",
    "@lexical/hashtag": "0.16.1",
    "@lexical/link": "0.16.1",
    "@lexical/list": "0.16.1",
    "@lexical/mark": "0.16.1",
    "@lexical/overflow": "0.16.1",
    "@lexical/plain-text": "0.16.1",
    "@lexical/react": "0.16.1",
    "@lexical/rich-text": "0.16.1",
    "@lexical/selection": "0.16.1",
    "@lexical/table": "0.16.1",
    "@lexical/utils": "0.16.1",
    "@lexical/dragon": "^0.16.1",
    "@lexical/headless": "^0.16.1",
    "@lexical/html": "^0.16.1",
    "@lexical/markdown": "^0.16.1",
    "@lexical/text": "^0.16.1",
    "@seolhun/root-ui": "^0.8.8-alpha.8",
    "@seolhun/root-ui-tailwind": "^0.8.8-alpha.8",
    "clsx": "^2.1.1",
    "i18next": "^23.12.1",
    "katex": "^0.16.1",
    "lexical": "^0.16.1",
    "lodash-es": "^4.17.21",
    "prettier": "^2.3.2",
    "react-error-boundary": "^4.0.13",
    "react-i18next": "^14.1.2",
    "tailwindcss": "^3.4.4"
  }
}
