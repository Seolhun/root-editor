#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hun/git/root-editor/node_modules/.pnpm/y-websocket@2.0.3_yjs@13.6.15/node_modules/y-websocket/bin/node_modules:/Users/hun/git/root-editor/node_modules/.pnpm/y-websocket@2.0.3_yjs@13.6.15/node_modules/y-websocket/node_modules:/Users/hun/git/root-editor/node_modules/.pnpm/y-websocket@2.0.3_yjs@13.6.15/node_modules:/Users/hun/git/root-editor/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hun/git/root-editor/node_modules/.pnpm/y-websocket@2.0.3_yjs@13.6.15/node_modules/y-websocket/bin/node_modules:/Users/hun/git/root-editor/node_modules/.pnpm/y-websocket@2.0.3_yjs@13.6.15/node_modules/y-websocket/node_modules:/Users/hun/git/root-editor/node_modules/.pnpm/y-websocket@2.0.3_yjs@13.6.15/node_modules:/Users/hun/git/root-editor/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../y-websocket/bin/server.cjs" "$@"
else
  exec node  "$basedir/../y-websocket/bin/server.cjs" "$@"
fi
